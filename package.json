{
  "name": "postcss-safe-curly-braces",
  "version": "0.1.0",
  "description": "PostCSS plugin that separates back-to-back closed curly braces `}}`, that might break certain templating engines like Handlebars or Vue.js",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "handlebars",
    "vuejs",
    "vue.js"
  ],
  "author": "Riccardo Bellingeri <riccardo.bellingeri@gmail.com>",
  "license": "MIT",
  "repository": "rbellingeri@gilt.com/postcss-separate-curly-braces",
  "bugs": {
    "url": "https://github.com/rbellingeri@gilt.com/postcss-separate-curly-braces/issues"
  },
  "homepage": "https://github.com/rbellingeri@gilt.com/postcss-separate-curly-braces",
  "dependencies": {
    "postcss": "latest"
  },
  "devDependencies": {
    "eslint": "3.x.x",
    "eslint-config-postcss": "2.x.x",
    "jest": "20.x.x"
  },
  "scripts": {
    "test": "jest && eslint *.js"
  },
  "eslintConfig": {
    "extends": "eslint-config-postcss",
    "env": {
      "jest": true
    }
  }
}
